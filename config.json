{
    "seed": 1,
    "lr": 0.001,
    "data": "custom",
    "num_workers": 1,
    "shuffle": true,
    "epochs": 2,
    "multiclass": true,
    "batch_size": 10,
    "num_workers": 2,
    "lr_step_size": 25,
    "lr_gamma": 0.001,
    "loss": { "l1": ["CrossEntropyLoss"]},
    "lr_finder": {
        "optimizer": {"lr": 1e-5},
        "range_test": {"end_lr":100, "step_mode": "exp"}
    },
    "optimizer": {
        "type": "Adam",
        "weight_decay": 1e-4
    },
    "scheduler": {
        "type": "ReduceLROnPlateau",
        "patience": 2
    },
    "lr_finder_use": false,
    "transformations": {
        "train": {
            "which": "albumentation",
            "what": [
            {
                "name": "Resize",
                "height":224,
                "width": 224, 
                "p": 1,
                "always_apply": true
            },
            {
                "name": "Cutout",
                "num_holes":1,
                "max_h_size": 80, 
                "max_w_size": 80,
                "always_apply": true
            },
            {
                "name": "Blur",
                "always_apply": false,
                "p": 0.2
            },
            {
                "name": "GaussNoise",
                "always_apply": false,
                "p": 0.2
            }
        ]
        },
        "test": {
            "which": "albumentation",
            "what": [
                {
                    "name": "Resize",
                    "height":224,
                    "width": 224, 
                    "p": 1,
                    "always_apply": true
                }
            ]
        }
    },
    "model": "mobilenet_v2",
    "model_initializer": {
       
    },
    "model_optimization": {
        "prunning": false
    },
    "log_dir": "/Users/anupgogoi/Documents/GitHub/S15AB"
}
